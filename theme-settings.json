{
    "theme": {
        "aside": {
            "border-radius": "6px",
            "border-style": "double",
            "border-width": "3px"
        },
        "border-radius": "0",
        "button": {
            "border-radius": "16px",
            "border-width": "1px",
            "border-style": "solid"
        },
        "code": {
            "border-radius": "16px",
            "border-width": "1px",
            "border-style": "solid"
        },
        "color": {
            "fill": {
                "dark": "rgb(20, 20, 22)",
                "light": "rgb(255, 255, 255)"
            },
            "documentation-intro-fill": "radial-gradient(circle at top, var(--color-documentation-intro-accent) 15%, rgb(17, 17, 17) 100%)",
            "documentation-intro-accent": "rgb(204, 204, 204)"
        },
        "icons": {
            "article": "/images/article.svg",
            "collection": "/images/collection.svg",
            "curly-brackets": "/images/curly-brackets.svg"
        }
    },
    "features": {
        "quickNavigation": {
            "enable": true
        }
    }
}
